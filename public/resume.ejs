<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.tailwindcss.com?plugins=forms,typography,aspect-ratio,line-clamp"></script>

    <title>
        Resume
    </title>
</head>

<body class="bg-orange-500 p-16">
    <script> document.addEventListener('contextmenu', event => event.preventDefault());</script>

    <div class="rounded-lg border-solid border-4 border-slate-500  p-4 bg-gradient-to-r from-indigo-200 via-red-200 to-yellow-100"
        id="main">
        <br />
        <h1 class="font-serif text-5xl font-semibold text-center">
            <%= form.name.toUpperCase() %>
        </h1>
        <div class="text-center text-sm">
            Email : <%= form.email %>
                ||
                Mobile Number : <%= form.mob %>
        </div>
        <br />
        <hr />
        <div>
            <br /><br />
            <div class="flex">
                <div class="mx-16 text-xl font-medium">



                    <!-- for Picture -->
                    <div class="max-w-xs">
                        <img class="object-cover"
                            src="data:image/jpeg;base64," />
                    </div>

                    <div class="max-w-xs object-center">
                        <img class="h-16"
                            src="https://camo.githubusercontent.com/fcd5a5ab2be5419d00fcb803f14c55652cf60696d7f6d9828b99c1783d9f14a3/68747470733a2f2f662e636c6f75642e6769746875622e636f6d2f6173736574732f393837332f3236383034362f39636564333435342d386566632d313165322d383136652d6139623137306135313030342e706e67" />
                    </div>

                    <br /><br />
                    Address : <br />
                    <div class="max-w-xs text-base">
                        <%= form.address %>
                    </div>

                </div>
                <div>
                    <div class="font-sans text-2xl font-italic ">About Me :</div>
                    <br />
                    <div class="mb-8 max-w-5xl">
                        <%= form.summary %>
                    </div>
                    <hr />
                    <br />

                    <!-- div for education -->
                    <div>
                        <h3 class="font-sans text-2xl font-italic "> Educational Qualifications : </h3>
                        <br />
                        <h4 class="font-sans text-xl font-italic mx-8">
                            <%= form.edu1.toUpperCase() %>
                        </h4>
                        <div class="mx-8">
                            <%= form.edu1Details %>
                        </div>
                        <br />
                        <h4 class="font-sans text-xl font-italic mx-8">
                            <%= form.edu2.toUpperCase() %>
                        </h4>
                        <div class="mx-8">
                            <%= form.edu2Details %>
                        </div>
                    </div>
                    <br /><br />
                    <!-- div for skills -->

                    <div>
                        <h3 class="font-sans text-2xl font-italic  "> Skills </h3>
                        <div class=" mx-8">
                            <%= form.skills %>
                        </div>
                    </div>
                    <br />

                    <!-- div for projects -->
                    <div class="my-8">
                        <h3 class="font-sans text-2xl font-italic "> Projects / Experince : </h3>
                        <br />
                        <h4 class="font-sans text-xl font-italic mx-8">
                            <%= form.project1.toUpperCase() %>
                        </h4>
                        <div class="mx-8">
                            <%= form.project1Desc %>
                        </div>
                        <br />
                        <h4 class="font-sans text-xl font-italic mx-8">
                            <%= form.project2.toUpperCase() %>
                        </h4>
                        <div class="max-w-3xl mx-8">
                            <%= form.project2Desc %>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <button onclick="window.print()">Print This Page</button>
</body>

</html>